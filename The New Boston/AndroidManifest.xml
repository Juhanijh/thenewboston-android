        <activity
            android:name=".Data"
            android:label="Quiz" >
        </activity>
        <activity
            android:name=".OpenedClass"
            android:label="Quiz" >
        </activity>
        <activity
            android:name=".AboutUs"
            android:label="@string/app_name"
